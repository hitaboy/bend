{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "820px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins/poppins-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins/poppins-700-normal.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/poppins/poppins-200-normal.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/poppins/poppins-900-normal.woff2"
							]
						}
					],
					"fontFamily": "Poppins, sans-serif",
					"name": "Poppins",
					"slug": "poppins"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--black)",
			"text": "var(--wp--preset--color--white)"
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json"
}